    "ssr-glue-plugin-server-vue-usehead": "^1.0.5",
    "vue": "^3.0.11",
    "vue-router": "^4.0.5"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.11",
    "@types/serve-static": "^1.13.9",
    "@vitejs/plugin-vue": "^1.2.1",
    "@yaquawa/vite-plugin-ssr": "file:..",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
